@model string

<div class="main">
    <table border=0 cellpadding=0 cellspacing=0 class="table table-bordered" id="tb_list">
        <thead>
            <tr>
                <td align="center" rowspan="2" style="width:40px"><input type="checkbox" title="选择" class="ui-selectInvert hidden" data-custom-params="isReverse:false"></td>
                <td align="center" rowspan="2">方案名</td>
                <td align="center" rowspan="1" colspan="2">缓存分钟</td>
                <td align="center" rowspan="2">最大列表页码</td>
                <td align="center" rowspan="2">排除参数</td>
                <td align="center" rowspan="2">排除关键词</td>
            </tr>
            <tr>
                <td align="center">栏目页</td>
                <td align="center">信息页</td>
            </tr>
        </thead>
        <tr class="item list-template-item" id="item_{id}" name="{name}">
            <td align="center" style="width:40px"><input type="checkbox" value="{id}" class="checked-item"></td>
            <td align="left"><span class="sp_name">{name}</span></td>
            <td align="center">{columnCacheMinutes}分钟</td>
            <td align="center">{detailCacheMinutes}分钟</td>
            <td align="center">{cacheMaxPage}</td>
            <td align="center">{nocacheUrlParams}</td>
            <td align="center">{nocacheUrlKeywords}</td>
        </tr>
    </table>

    <div class="submit-footerbar">
        <label class="control-label"></label>
        <div class="controls">
            <button type="button" class="btn btn-primary btn-sm ui-closeDialog">确定</button>
            <button type="button" class="btn btn-sm btn-default ui-closeDialog">关闭</button>
        </div>
    </div>
</div>

<script src="/Js/selectData.js" type="text/javascript"></script>
<script type="text/javascript">
    function LoadedInit() {
        SetSelectedDisabled();
    }
    function LoadData() {
        var ajaxUrl = "@Url.ActionUrl("PageListData")?_isload=1"
        $("#tb_list").ListBind({url:ajaxUrl,callBack:"LoadedInit" });
    }

    LoadData();
</script>