@{
PageInfo pageInfo = new PageInfo()
    {
        PageSize =8,//每页显示8条数据
        CurrentPage = (int)ViewBag.CurrentPage //当前页码，系统预设
    };
}
<div class="container mycontainer padding-top-20 clearFix">
    <div class="rightmain-box float-left padding-bottom-30 padding-right-30">
        @Html.Partial("ArticleListModel",Html.InfoDataList(new {Table="Article",OrderBy = "thedate desc" },null,null,pageInfo))
        @Html.Partial("PagebreakPartial", pageInfo) 
    </div>
    <div class="leftnav-box float-left">
        @Html.Partial("leftNavPartial")
    </div>
</div>

